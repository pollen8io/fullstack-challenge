{
  "name": "fullstack-challenge-example",
  "homepage": "https://github.com/pollen8io/fullstack-challenge/tree/example",
  "description": "An example scaffold of this repository's challenge",
  "main": "./index.mjs",
  "scripts": {
    "dev": "nodemon index.mjs",
    "serve": "cd vendor && vue-cli-service serve",
    "build": "cd vendor && vue-cli-service build",
    "lint": "cd vendor && vue-cli-service lint"
  },
  "engines": {
    "node": ">=14.15.4"
  },
  "nodemonConfig": {
    "delay": "500",
    "verbose": true,
    "ignore": [
      "vendor/"
    ],
    "execMap": {
      "js": "node --inspect --trace-warnings -r dotenv/config",
      "mjs": "node --inspect --trace-warnings -r dotenv/config"
    }
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "cookie-parser": "^1.4.5",
    "debug": "^4.1.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-respond": "^1.5.2",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.21.5",
    "lodash": "^4.17.19",
    "moment": "^2.24.0",
    "passport": "^0.4.1",
    "pg": "^8.0.3",
    "vue": "^2.6.10",
    "vue-router": "^3.4.9",
    "vuex": "^3.2.0",
    "ws": "^7.2.3"
  },
  "devDependencies": {
    "@babel/core": "^7.11.4",
    "@babel/eslint-parser": "^7.11.4",
    "@babel/eslint-plugin": "^7.11.3",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-proposal-private-methods": "^7.10.4",
    "@babel/plugin-syntax-class-properties": "^7.10.4",
    "@babel/preset-env": "^7.11.0",
    "@vue/cli-plugin-babel": "^4.3.1",
    "@vue/cli-plugin-eslint": "^4.3.1",
    "@vue/cli-plugin-unit-jest": "^4.2.3",
    "@vue/cli-service": "^4.2.3",
    "@vue/eslint-config-standard": "^5.1.2",
    "@vue/test-utils": "^1.0.3",
    "eslint": "^7.7.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "eslint-plugin-vue": "^6.2.2",
    "jest": "^26.6.0",
    "jest-environment-node": "^26.0.1",
    "nodemon": "^2.0.6",
    "sass-loader": "^9.0.2",
    "vue-loader": "^15.9.1",
    "vue-template-compiler": "^2.6.11",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11",
    "webpack-dev-middleware": "^3.7.2"
  },
  "optionalDependencies": {
    "fsevents": "^2.1.3"
  }
}
